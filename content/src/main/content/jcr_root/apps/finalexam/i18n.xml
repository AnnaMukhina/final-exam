                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Date"/>
        <find
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Find event"/>
        <latitude
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Latitude"/>
        <longitude
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Longitude"/>
        <place
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Place"/>
        <tour
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Tour"/>
        <find
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Find"/>
    </en>
    <ru jcr:primaryType="nt:folder" jcr:mixinTypes="mix:language" jcr:language="ru">
        <add
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Добавить событие"/>
