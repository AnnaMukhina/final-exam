                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Город"/>
        <date
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Дата"/>
        <find
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Найти событие"/>
        <latitude
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Широта"/>
        <longitude
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Долгота"/>
        <place
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Место проведения"/>
        <tour
                jcr:mixinTypes="[sling:Message]"
                jcr:primaryType="nt:folder"
                sling:message="Турне"/>
