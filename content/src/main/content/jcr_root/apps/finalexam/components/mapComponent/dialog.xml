<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Dialog"
          title="Map Component"
          xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1 jcr:primaryType="cq:Panel" title="Map Component">
                <items jcr:primaryType="cq:WidgetCollection">
                    <height jcr:primaryType="nt:unstructured" fieldLabel="Height of the map"
                            name="./height" validateOnBlur="true" xtype="textfield"/>
                    <latitude jcr:primaryType="nt:unstructured" fieldLabel="Latitude of the center"
                          name="./latitude" validateOnBlur="true" xtype="textfield"/>
                    <longitude jcr:primaryType="nt:unstructured" fieldLabel="Longitude of the center"
                              name="./longitude" validateOnBlur="true" xtype="textfield"/>
                    <zoom jcr:primaryType="nt:unstructured" fieldLabel="Zoom"
                               name="./zoom" validateOnBlur="true" xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>